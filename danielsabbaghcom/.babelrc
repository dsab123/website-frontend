{
  "sourceMap": true,
  "sourceRoot": "src",
  "moduleIds": false,
  "comments": false,
  "compact": false,
  "presets": [
    "env"
  ],
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types",
    "transform-regenerator"
  ],
  "env" : {
    "test": {
      "presets": ["env"],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "development": {
      "presets": ["env"],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
